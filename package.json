  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "babel-jest": "^24.7.1",
    "concurrently": "^4.1.1",
    "jest": "^24.8.0",
